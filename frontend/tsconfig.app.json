{
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc/app",
    "module": "esnext",
    "types": [],
    "target": "es2022",
    "moduleResolution": "node",
    "importHelpers": true,
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "strict": true
  },
  "files": ["src/main.ts"],
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules", "dist"]
}

