{
  "name": "crialt-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "ng serve",
    "dev": "ng serve -c development",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "dev:ssr": "ng run crialt-frontend:serve-ssr:development",
    "build:ssr": "ng build && ng run crialt-frontend:server:production",
    "prerender": "ng run crialt-frontend:prerender:production",
    "serve:ssr": "node dist/crialt-frontend/server/main.js",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix"
  },
  "dependencies": {
    "@angular/animations": "^19.0.0",
    "@angular/cdk": "^19.0.0",
    "@angular/common": "^19.0.0",
    "@angular/core": "^19.0.0",
    "@angular/forms": "^19.0.0",
    "@angular/material": "^19.0.0",
    "@angular/platform-browser": "^19.0.0",
    "@angular/platform-server": "^19.0.0",
    "@angular/router": "^19.0.0",
    "@angular/ssr": "^19.2.15",
    "chart.js": "^4.4.4",
    "express": "^4.18.2",
    "ng2-charts": "^6.0.1",
    "ngx-mask": "^20.0.3",
    "ngx-spinner": "^17.0.0",
    "rxjs": "^7.8.2",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.0",
    "@angular/cli": "^19.0.0",
    "@angular/compiler-cli": "^19.0.0",
    "@types/express": "^4.17.17",
    "browser-sync": "^3.0.0",
    "typescript": "^5.5.4"
  }
